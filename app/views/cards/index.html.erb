<head>
  <title>FocusField</title>
  <%= render partial: "layouts/head_elements" %>
</head>
<body>
  <%= render partial: "layouts/navbar" %>
  <div class="container">
    <div class="content">
      <h1>Cards#index</h1>
      <p>Find me in app/views/cards/index.html.erb</p>
    </div>
  </div>
  <div class="box">
    <div class="container">
      <% @resources.each do |resource| %>
        <div class="media">
          <div class="media-content">
            <div class="content">
              <%= resource.name %>
              <%= link_to "edit", edit_card_path(resource.id_hex) %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</body>
